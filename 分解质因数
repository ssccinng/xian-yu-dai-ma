#include<stdio.h>
	int main()
	{
		int x;
		int t; 
		int y;
		int is=1; 
		scanf("%d",&x);
		y=x;
		if(2<=x==1==x<=100000){
			for(t=2;t<=y;){
				if(y%t==0){
					if(y!=t){
					if(is){
					printf("%d=",x);
					is=0;
					}
					printf("%dx",t);
					y=y/t;}else{
					printf("%d",t);
					y=y/t;
					} 
				}else{
					t++;
				}
			}
		}
		
		return 0;
	}
