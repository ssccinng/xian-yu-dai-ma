#include<stdio.h>
	int main(){
	int x;
	int counter=0;
	int sum=0;
	int t;
	int n,m;
		scanf("%d %d",&n,&m);
		for(x=2;counter<=200;x++){
		if(x==2){
			counter++;
			if(n==1){
			sum+=2;
			}
		}
		for(t=2;t<x;t++){
			if(x%t==0){
				break;}
			if(t==x-1){
				counter++;
			if(n<=counter==1==counter<=m){
				sum+=x;}else{
				break;}
			}
	}}
		printf("%d",sum);
	return 0;
	}
	
